{
  "project_title": "Project Title",
  "project_slug": "{{ cookiecutter.project_title|lower|replace(' ', '-') }}",
  "description": "A new project using Plone 6.",
  "author": "Plone Foundation",
  "email": "collective@plone.org",
  "python_package_name": "{{ cookiecutter.project_slug|replace(' ', '')|replace('-', '_')|replace('.', '') }}",
  "volto_addon_name": "volto-{{ cookiecutter.python_package_name|replace('_', '-') }}",
  "python_test_framework": [
    "pytest",
    "unittest"
  ],
  "__python_package_name_upper": "{{ cookiecutter.python_package_name|pascal_case }}",
  "plone_version": "{{ '6.0.7' | latest_plone }}",
  "volto_version": "{{ '16.0.0' | latest_volto }}",
  "__node_version": "{{ cookiecutter.volto_version | node_version }}",
  "volto_generator_version": "{{ cookiecutter.volto_version | latest_volto_generator }}",
  "language_code": [
    "en",
    "de",
    "es",
    "pt-br",
    "nl",
    "fi"
  ],
  "github_organization": "collective",
  "container_registry": [
    "GitHub",
    "Docker Hub"
  ],
  "__prompts__": {
    "project_title": "Project Title",
    "project_slug": "Project Slug (Used for repository id)",
    "description": "Project Description",
    "author": "Author",
    "email": "Author E-mail",
    "python_package_name": "Python Package Name",
    "volto_addon_name": "Volto Addon Name",
    "python_test_framework": {
      "__prompt__": "Choose a Python Test Framework",
      "pytest": "pytest",
      "unittest": "unittest"
    },
    "plone_version": "Plone Version",
    "volto_version": "Volto Version",
    "volto_generator_version": "Volto Generator Version",
    "language_code": {
      "__prompt__": "Language",
      "en": "English",
      "de": "Deutsch",
      "es": "Español",
      "pt-br": "Português (Brasil)",
      "nl": "Nederlands",
      "fi": "Suomi"
    },
    "github_organization": "GitHub Organization",
    "container_registry": {
      "__prompt__": "Container Registry",
      "GitHub": "GitHub Container Registry",
      "Docker Hub": "Docker Hub"
    }
  },
  "__docker_image_prefix": "{{ cookiecutter.container_registry | docker_image_prefix }}",
  "__profile_version": "{% now 'utc', '%Y%m%d001' %}",
  "__profile_language": "{{ cookiecutter.language_code|gs_language_code }}",
  "__locales_language": "{{ cookiecutter.language_code|locales_language_code }}",
  "__generator_date_short": "{% now 'utc', '%Y-%m-%d' %}",
  "__generator_date_long": "{% now 'utc', '%Y-%m-%d %H:%M:%S' %}",
  "__generator_signature": "This was generated by [cookiecutter-plone-starter](https://github.com/collective/cookiecutter-plone-starter) on {{ cookiecutter.__generator_date_long }}",
  "_extensions": [
    "local_extensions.node_version",
    "local_extensions.docker_image_prefix",
    "local_extensions.pascal_case",
    "local_extensions.locales_language_code",
    "local_extensions.gs_language_code",
    "local_extensions.latest_volto",
    "local_extensions.latest_volto_generator",
    "local_extensions.latest_plone"
  ]
}
